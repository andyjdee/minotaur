{
  "name": "Minotaur",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "src/main.js",
  "scripts": {
    "build-css": "node-sass-chokidar src/scss/style.scss -o src/scss/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "BROWSER=none nf start -p 3000",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node src/bootstrap",
    "react-start": "react-scripts start"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "classnames": "^2.2.5",
    "electron": "^1.8.3",
    "flag-icon-css": "^3.0.0",
    "font-awesome": "^4.7.0",
    "foreman": "^2.0.0",
    "jquery": "1.9.1",
    "npm-run-all": "^4.1.2",
    "popper.js": "^1.12.9",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "react-transition-group": "1.1.2",
    "reactstrap": "^4.8.0",
    "simple-line-icons": "^2.4.1"
  },
  "devDependencies": {
    "electron-builder": "^20.4.0",
    "node-sass-chokidar": "0.0.3"
  },
  "build": {
    "appId": "com.aveo.minotaur",
    "win": {
      "iconUrl": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
    },
    "directories": {
      "buildResources": "public"
    }
  }
}
